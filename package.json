{
  "name": "nam-trainer-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:backend": "uvicorn backend.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:backend:debug": "uvicorn backend.debug_main:app --reload --host 0.0.0.0 --port 8000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "dev": "vite",
    "dev:full": "concurrently -k \"npm run dev:backend\" \"npm run dev\""
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^9.2.1",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1"
  }
}
